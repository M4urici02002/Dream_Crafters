<%- include('includes/navbar.ejs') %>
<%- include('includes/sidebar.ejs') %>
<%- include('includes/subsidebarSimple.ejs') %>

<div class="main p-3">
  <div class="container">
    <div class="col">
      <% for (let re of resena) { %>
        <div class="card">
          <div class="card-header">
            <%=re.Titulo%>
          </div>
          <div class="card-body">
            <h5 class="card-title"><%=re.Titulo%></h5>
            <p class="card-text"><%=re.Descripcion%></p>
            <p class="card-text"><%=re.FechaContestacion%></p>
            <p class="card-text"> Estrellas: <%=re.Rating%></p>    
          </div>
        </div>
      <% } %>
    </div>
  </div>
</div>
<%- include('includes/footer.ejs') %>