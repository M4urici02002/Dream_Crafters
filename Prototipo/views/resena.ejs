<%- include('includes/navbar.ejs') %>
   <main>
        <aside style="float: left; width: 20%;">
            <%- include('includes/sidebar.ejs') %>
        </aside>
        <section style="float: left; width: 15%;">
            <%- include('includes/subsidebar.ejs') %>
        </section>

        <!--Prototipo de contenedor de reseÃ±a-->
        <section style="float: right; width: 65%; padding-top: 15px; padding-right: 30px;">
          <div class="col">
            <% for (let re of resena) { %>
                <div class="card">
                    <div class="card-header">
                        <%=re.producto%>
                    </div>
                    <div class="card-body">
                      <h5 class="card-title"><%=re.titulo%></h5>
                      <p class="card-text"><%=re.nombre%></p>
                      <p class="card-text"><%=re.fecha%></p>
                      <p class="card-text"><%=re.estrellas%></p>
                      
                    </div>
                  </div>
            <% } %>
        </div>
        </section>
    </main>
